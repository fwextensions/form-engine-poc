{
  "name": "form-engine-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=schema-viewer",
    "build": "npm run build --workspace=schema-viewer",
    "start": "npm run start --workspace=schema-viewer",
    "lint": "npm run lint --workspace=schema-viewer",
    "lint:all": "npm run lint --workspaces --if-present",
    "build:all": "npm run build --workspaces --if-present"
  }
}
